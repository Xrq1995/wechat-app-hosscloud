/**app.wxss**/
@import "weui-wxss/dist/style/weui.wxss";

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.ub {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    display: box;
    position:relative;
}
/* zw add main.css*/
page {
  background-color: #EEE;
}
.page{
  font-size: 15px;
  color: #333;
  background-color: #FFF;
}
.weui-cells {
   font-size:15px;
  }
  .weui-bar__item_on{
    color: #6494F6 !important;
  }
button[type=primary] {
  width: 98%;
  height: 80rpx;
  background-color: #6494F6 !important;
  color: #fff;
  font-size: 32rpx;
  margin-bottom:20rpx; 
}
.weui-navbar__slider{
  background-color: #6494F6 !important;
}
.weui-cells__swiper{
  padding: 2px;
}
image {
  width: 100%;
  height: 100%;
  display:inline-block;
  overflow:hidden;
}


/*jcj样式  */
.center_center {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    margin: auto;
}

a {
    color: #333;
    text-decoration: none;
}

.hide-input {
    width: 0;
    height: 0;
    overflow: hidden;
    visibility: hidden;
}

input::-webkit-input-placeholder,
textarea::-webkit-input-placeholder {
    color: #aaa;
}

input:-moz-placeholder,
textarea:-moz-placeholder {
    color: #aaa;
}

input::-moz-placeholder,
textarea::-moz-placeholder {
    color: #aaa;
}

input:-ms-input-placeholder,
textarea:-ms-input-placeholder {
    color: #aaa;
}

ul,
li {
    list-style: none;
}


/*单行变成省略号*/

.cutText {
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    word-break: break-all;
}


/*两行变成省略号*/

.cutText-2 {
    text-overflow: ellipsis;
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}


/*清除浮动*/

.clearfix {
    zoom: 1
}

.clearfix:after {
    height: 0;
    visibility: hidden;
    display: block;
    clear: both;
    content: "";
}


/*
 *input框 灰色
 */

input::-webkit-input-placeholder {
    /* WebKit browsers */
    color: #999;
}

input:-moz-placeholder {
    /* Mozilla Firefox 4 to 18 */
    color: #999;
}

input::-moz-placeholder {
    /* Mozilla Firefox 19+ */
    color: #999;
}

input:-ms-input-placeholder {
    /* Internet Explorer 10+ */
    color: #999;
}


/**
 *     改变placeholder样式
 */

::-webkit-input-placeholder {
    color: #262626;
}

::-moz-placeholder {
    color: #262626;
}

::-ms-input-placeholder {
    color: #262626;
}


