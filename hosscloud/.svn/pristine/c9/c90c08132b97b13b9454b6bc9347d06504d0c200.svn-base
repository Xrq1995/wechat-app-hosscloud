
.containers{
  width: 100vw;
  height: 100vh;
}

.logo{
  display: block;
  width: 40vw;
  height: 40vw;
  padding-top: 10vh;
  margin: auto
}


.search-container {
  /* width: 100%; */
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #42BD56;
  color: #FFF;
  height: 80rpx;
  padding: 10rpx 20rpx;
  z-index: 100;
}
.search-container image{
  width: 40rpx;
  height: 40rpx;
  display: block;
  color: white;
  padding-left: 20px;
  line-height: 20px;
}
 .search-container input {
  background: #FFF;
  color: #AAA;
  padding: 5px 10rpx;
  height: 40rpx;
  width: 100%;
  border-radius: 8rpx;
}
